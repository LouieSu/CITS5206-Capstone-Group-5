<?xml version="1.0" encoding="UTF-8"?>
<rulesets>

    <!-- Courses or programmes (currently MIT and MDS only). Attributes may be expanded as needed.-->
    <programme>
        <code>MIT</code>
        <name>Master of Information Technology</name>
    </programme>

    <programme>
        <code>MDS</code>
        <name>Master of Data Science</name>
    </programme>


    <!-- Ruleset part -->
    <!-- 1. MIT, 2025 rule set-->
    <ruleset>
        <code>MIT-2025</code>
        <name>Master of Information Technology, 2025 ruleset</name>
        <programmeCode>MIT</programmeCode>
        <totalPoints>96</totalPoints>


        <sections>

            <section>
                <code>conversion</code>
                <name>Conversion</name>
                <description>
                    Students who have completed degree studies in a non-cognate area, or equivalent as recognised by the School, must complete relevant conversion units up to the value of 24 points as determined by the School upon offer of admission and by the scope of a student's prior study. Students choose either CITS2002 or CITS2005.
                </description>

                <!-- For front-end display purposes -->
                <units>
                    CITS1003, CITS1401, CITS1402, CITS2002, CITS2005
                </units>

                <!-- All conditions must be satisfied -->
                <conditions>

                    <!-- 18 points from these two units or take all three units-->
                    <condition>
                        <!-- Starting with a capital letter. Can be UnitPoint/LevelPoint/UnitNumber/Unit/TotalPoint/Programme/LevelNumber -->
                        <!-- In a rule set, only UnitPoint/LevelPoint will be used. Others are designed for describing prerequisites -->
                        <type>UnitPoint</type>
                        <points>18</points>
                        <units>CITS1003, CITS1401, CITS1402</units>
                    </condition>

                    <!-- 6 points from these two units.-->
                    <condition>
                        <type>UnitPoint</type>
                        <points>6</points>
                        <units>CITS2002, CITS2005</units>

                        <!-- Description is optional-->
                        <description></description>

                    </condition>
                </conditions>
            </section>

            <section>
                <code>core</code>
                <name>Core</name>
                <descriptio>Take all units (24 points)</descriptio>

                <units>CITS4401, CITS5206, CITS5505, PHIL4100</units>

                <conditions>
                    <condition>
                        <type>UnitPoint</type>
                        <points>24</points>
                        <units>CITS4401, CITS5206, CITS5505, PHIL4100</units>
                    </condition>
                </conditions>
            </section>

            <section>
                <code>optionA</code>
                <name>Option - Group A</name>
                <descriptio>Take unit(s) to the value of 24 points, including a minimum of 12 points at Level 5.</descriptio>

                <units>AUTO4508, CITS4009, CITS4012, CITS4402, CITS4403, CITS4404, CITS4407, CITS4419, CITS5014, CITS5015, CITS5017, CITS5501, CITS5503, CITS5504, CITS5506, CITS5507, CITS5508, ENVT4411, GENG5507, INMT5518, INMT5526, MGMT5504, SVLG5001</units>

                <conditions>
                    <condition>
                        <type>UnitPoint</type>
                        <points>24</points>
                        <units>AUTO4508, CITS4009, CITS4012, CITS4402, CITS4403, CITS4404, CITS4407, CITS4419, CITS5014, CITS5015, CITS5017, CITS5501, CITS5503, CITS5504, CITS5506, CITS5507, CITS5508, ENVT4411, GENG5507, INMT5518, INMT5526, MGMT5504, SVLG5001</units>
                    </condition>
                    <condition>
                        <type>LevelPoint</type>
                        <level>5</level>
                        <points></points>
                    </condition>
                </conditions>
            </section>

            <!-- Specialisation is a special type of section with the same structure -->
            <specialisation>
                <code>ac</code>
                <name>Applied Computing specialisation</name>
                <descriptio>Take unit(s) to the value of 24 points, including a minimum of 12 points at Level 5.</descriptio>

                <units>CITS4009, CITS4012, CITS4402, CITS4403, CITS4404, CITS4407, CITS4419, CITS5017, CITS5501, CITS5503, CITS5504, CITS5506, CITS5507, CITS5508</units>

                <conditions>
                    <condition>
                        <type>UnitPoint</type>
                        <points>24</points>
                        <units>CITS4009, CITS4012, CITS4402, CITS4403, CITS4404, CITS4407, CITS4419, CITS5017, CITS5501, CITS5503, CITS5504, CITS5506, CITS5507, CITS5508</units>
                    </condition>
                    <condition>
                        <type>LevelPoint</type>
                        <level>5</level>
                        <points></points>
                    </condition>
                </conditions>
            </specialisation>

            <specialisation>
                <code>ai</code>
                <name>Artificial Intelligence specialisation</name>
                <descriptio>Take all units (24 points):</descriptio>

                <units>CITS4012, CITS4404, CITS5017, CITS5508</units>

                <conditions>
                    <condition>
                        <type>UnitPoint</type>
                        <points>24</points>
                        <units>CITS4012, CITS4404, CITS5017, CITS5508</units>
                    </condition>
                </conditions>
            </specialisation>


            <specialisation>
                <code>ss</code>
                <name>Software Systems specialisation</name>
                <descriptio>Take all units (24 points):</descriptio>

                <units>CITS5501, CITS5503, CITS5506, CITS5507</units>

                <conditions>
                    <condition>
                        <type>UnitPoint</type>
                        <points>24</points>
                        <units>CITS5501, CITS5503, CITS5506, CITS5507</units>
                    </condition>
                </conditions>
            </specialisation>
            
        </sections>
        
    </ruleset>


    <!-- 2. MDS 2025 rule set-->
<ruleset>
    <code>MDS-2025</code>
    <name>Master of Data Science, 2025 ruleset</name>
    <programmeCode>MDS</programmeCode>
    <totalPoints>96</totalPoints>
    <sections>
        <!-- Conversion Section -->
        <section>
            <code>conversion</code>
            <name>Conversion</name>
            <description>
                Students from non-cognate backgrounds must complete up to 24 points of conversion units as advised by the School.
            </description>
            <units>
                CITS1401, CITS1402, STAT2401, STAT2402
            </units>
            <conditions>
                <condition>
                    <type>UnitPoint</type>
                    <points>24</points>
                    <units>CITS1401, CITS1402, STAT2401, STAT2402</units>
                </condition>
            </conditions>
        </section>

        <!-- Core Section -->
        <section>
            <code>core</code>
            <name>Core</name>
            <description>Take all units (48 points)</description>
            <units>
                CITS4009, CITS4012, CITS4407, CITS5504, CITS5508, CITS5553, STAT4064, STAT4066
            </units>
            <conditions>
                <condition>
                    <type>UnitPoint</type>
                    <points>48</points>
                    <units>CITS4009, CITS4012, CITS4407, CITS5504, CITS5508, CITS5553, STAT4064, STAT4066</units>
                </condition>
            </conditions>
        </section>

        <!-- Option Group A -->
        <section>
            <code>optionA</code>
            <name>Option - Group A</name>
            <description>Take units to the value of 24 points, including a minimum of 12 points at Level 5</description>
            <units>
                BUSN5003, CITS4402, CITS4403, CITS4404, CITS4419, CITS5014, CITS5015, CITS5017, CITS5503, CITS5505,
                CITS5506, CITS5507, ECON5570, GENG5505, INMT5526, MGMT5504, PHYS4021, PUBH4401, PUBH5769, PUBH5785,
                PUBH5802, STAT4063, STAT4065, STAT5061, SVLG5001
            </units>
            <conditions>
                <condition>
                    <type>UnitPoint</type>
                    <points>24</points>
                    <units>BUSN5003, CITS4402, CITS4403, CITS4404, CITS4419, CITS5014, CITS5015, CITS5017, CITS5503, CITS5505, CITS5506, CITS5507, ECON5570, GENG5505, INMT5526, MGMT5504, PHYS4021, PUBH4401, PUBH5769, PUBH5785, PUBH5802, STAT4063, STAT4065, STAT5061, SVLG5001</units>
                </condition>
                <condition>
                    <type>LevelPoint</type>
                    <level>5</level>
                    <points>12</points>
                </condition>
            </conditions>
        </section>
    </sections>
</ruleset>


</rulesets>
